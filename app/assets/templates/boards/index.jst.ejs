
<div class="clearfix">
  <ul>
    <% boards.each(function(board) { %>
      <li class="list-board"><a href="#/<%= board.id %>">
        <%= board.escape("title") %>
      </a>
      <button class="btn-delete-board" data-id="<%= board.id %>">
        Delete
      </button>
      </li>
    <% }); %>
      <li class="new-board-form list-board">
        <button class="btn-create-board">Create</button>
      </li>
  </ul>
</div>

<button class=".btn-refresh-board">Refresh</button>

